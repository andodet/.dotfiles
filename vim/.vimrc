filetype plugin indent on

" show existing tab with 4 spaces width
set tabstop=4

" when indenting with '>', use 4 spaces width
set shiftwidth=4

" On pressing tab, insert 4 spaces
set expandtab
set number!
" set number

set colorcolumn=120
highlight ColorColumn ctermbg=59

" Set gruvbox theme
set background=dark

set relativenumber
set hlsearch
set laststatus=2
set clipboard=unnamed
set noswapfile
